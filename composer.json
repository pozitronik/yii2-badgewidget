{
	"name": "pozitronik/yii2-badgewidget",
	"description": "Powerful badges for YII2",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension",
		"widget",
		"badges"
	],
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=8.0",
		"pozitronik/yii2-helpers": "^1.0.5"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\widgets\\": "src"
		}
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
